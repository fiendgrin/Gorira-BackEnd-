
@{
    ViewData["Title"] = "Home";
}
<!--Main Start-->
<main id="homeMain">
    <!--Swiper Start-->
    <section class="swiper topSlider">
        <!-- Additional required wrapper -->
        <div class="gradient"></div>
        <div class="swiper-wrapper">
            <!-- Slides -->
            <div class="swiper-slide">
                <div class="gradient"></div>

                <a href="tracks.html" class="slide1 slidebtn">Get Tracks Now</a>
                <div class="slide1 slidetext">
                    <p>Easy Access To Your Favorite Beats</p>
                </div>

                <img src="~/assets/images/slider/easy-access.png" alt="" />
            </div>

            <div class="swiper-slide">
                <div class="gradient"></div>

                <a href=""> class="slide1 slidebtn">Get Tracks Now</a>

                <div class="slide2 slidetext">
                    <p>Make Money From Home</p>
                </div>
                <img src="~/assets/images/slider/work-from-home.jpg" alt="" />
            </div>

            <div class="swiper-slide">
                <div class="gradient"></div>

                <a href=""> class="slide1 slidebtn">Get Tracks Now</a>

                <div class="slide3 slidetext">
                    <p>Become One Of The Greatest</p>
                </div>
                <img src="~/assets/images/slider/producers.jpg" alt="" />
            </div>
        </div>
    </section>
